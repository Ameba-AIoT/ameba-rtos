#

#
# < CONFIG TrustZone
#

#
# < CONFIG Link Option
#
CONFIG_IMG1_FLASH=y
# CONFIG_IMG1_SRAM is not set
CONFIG_IMG2_FLASH=y
# CONFIG_IMG2_PSRAM is not set
# CONFIG_PSRAM_AS_HEAP is not set

#
# < CONFIG Mass Production
#
# CONFIG_MP_INCLUDED is not set

#
# < CONFIG Shell CMD
#
CONFIG_SUPPORT_ATCMD=y
CONFIG_ATCMD_IO_LOGUART=y
# CONFIG_ATCMD_IO_UART is not set
# CONFIG_LONGER_CMD is not set

#
# < CONFIG VFS
#
CONFIG_VFS_LITTLEFS_INCLUDED=y
# CONFIG_VFS_FATFS_INCLUDED is not set

#
# < CONFIG OTA OPTION
#
# CONFIG_UPGRADE_BOOTLOADER is not set

#
# < CONFIG WIFI
#
CONFIG_WLAN_MENU=y
CONFIG_KM0_NP_KM4_AP=y
# CONFIG_SPI_FULLMAC is not set
# CONFIG_SDIO_FULLMAC is not set
# CONFIG_HIGH_TP_TEST is not set
CONFIG_WIFI_FW_EN=y
# CONFIG_WIFI_CSI_ENABLE is not set
# CONFIG_WIFI_ANTDIV_ENABLE is not set
# CONFIG_WIFI_11K_ENABLE is not set
# CONFIG_WIFI_11V_ENABLE is not set
# CONFIG_WIFI_11R_ENABLE is not set
# CONFIG_WIFI_SPEAKER_ENABLE is not set

#
# < CONFIG BT
#
# CONFIG_BT_MENU is not set

#
# < CONFIG LWIP
#
# CONFIG_LWIP_DEBUG is not set

#
#

#
# < MENUCONFIG FOR KM0 CONFIG
#

#
# < CONFIG CHIP
#
CONFIG_AMEBADPLUS=y
ARM_CORE_CM0=y
# CONFIG_FPGA is not set
CONFIG_AMEBADPLUS_A_CUT=y

#
# < CONFIG OS
#
CONFIG_KERNEL=y
PLATFORM_FREERTOS=y
TASK_SCHEDULER_DISABLED=0

#
# < CONFIG FUNCTION TEST
#
# CONFIG_PER_TEST is not set

#
# < Build Option
#
CONFIG_TOOLCHAIN_ASDK=y
# CONFIG_TOOLCHAIN_ARM_GCC is not set
# CONFIG_LINK_ROM_LIB is not set
CONFIG_LINK_ROM_SYMB=y

#
CONFIG_INIC_INTF_IPC=y
CONFIG_WLAN=y
CONFIG_AS_INIC_NP=y
CONFIG_FW_DRIVER_COEXIST=y
