include $(MAKE_INCLUDE_GEN)

.PHONY: all clean

#*****************************************************************************#
#                               VARIABLES                                     #
#*****************************************************************************#

#*****************************************************************************#
#                               OBJECT FILE LIST                              #
#*****************************************************************************#
OBJS = 

#*****************************************************************************#
#                               INCLUDE DEPENDENCY                            #
#*****************************************************************************#
-include $(OBJS:.o=.d)

#*****************************************************************************#
#                        RULES TO GENERATE TARGETS                            #
#*****************************************************************************#
all: CORE_TARGETS
	make -C amebadplus_bsp all
	make -C bt_coex all
	make -C bt_inic all

#*****************************************************************************#
#              GENERATE OBJECT FILE                                           #
#*****************************************************************************#
CORE_TARGETS: $(OBJS)

#*****************************************************************************#
#              CLEAN GENERATED FILES                                          #
#*****************************************************************************#
clean:
	make -C amebadplus_bsp clean
	make -C bt_coex clean
	make -C bt_inic clean

-include $(DEPS)
