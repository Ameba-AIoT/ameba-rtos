comment "--- --------------CPU config Start------------"

config SUPPORT_CPU_MVE_FOR_KM4
    def_bool y

config SUPPORT_DSP_TABLE_ROM_FOR_KM4
    def_bool y

menu "CONFIG CHIP"
    config CPU_HAS_FPU_FOR_KM4
        bool
        default y

    choice
        prompt "ArmCore"
        default ARM_CORE_CM4_FOR_KM4
        config ARM_CORE_CM4_FOR_KM4
            bool "ARM_CM4"
    endchoice

    choice
        prompt "Chip Version"
        default AMEBASMART_B_CUT_FOR_KM4
        config AMEBASMART_B_CUT_FOR_KM4
            bool "AMEBASMART_B_Cut"
    endchoice
endmenu
