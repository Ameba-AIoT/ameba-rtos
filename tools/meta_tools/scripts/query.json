{
    "info": {
        "brief": "Realtek Loong FreeRTOS SDK",
        "soc": "Ameba",
        "os": "FreeRTOS",
        "vcs": "repo",
        "meta": "loong.py",
        "debuggerArgs":  [
            "-iex","set rtos-aware 0",
            "-iex","maintenance set target-async on",
            "-iex","set debug remote 1"
        ],
	"overrideLaunchCommands": [
            "mem 0xa5a5a5a4 0xa5a5a5a6 none",
            "set mem inaccessible-by-default off",
            "monitor reset 0",
            "monitor sleep 20",
            "monitor reset 0",
            "monitor sleep 20",
            "flushregs"
        ]
    },
    "apps": {
        "include": [
            "amebad_gcc_project/project_km4/src",
            "amebadplus_gcc_project/project_km4/src",
            "amebalite_gcc_project/project_km4/src"
        ]
    },
    "devices": {
        "amebadplus": {
            "chip": "AmebaDplus",
            "path": "amebadplus_gcc_project"
        },
        "amebalite": {
            "chip": "AmebaLite",
            "path": "amebalite_gcc_project"
        }
    },
    "chips": {
        "AmebaDplus": {
            "core": "Cortex-M33",
            "target": "project_km4/asdk/make/target_img2_km4.axf",
            "toolchain": "asdk-10.3.1",
            "jlinkScript": "utils/jlink_script/AP2_KM4.JLinkScript"
        },
        "AmebaLite": {
            "core": "Cortex-M33",
            "target": "project_km4/asdk/make/target_img2_km4.axf",
            "toolchain": "asdk-10.3.1",
            "jlinkScript": "utils/jlink_script/AP0_KM4.JLinkScript"
        }
    }
}
