#

#
# < CONFIG DSP Enable
#
# CONFIG_DSP_EN is not set

#
# < CONFIG TrustZone
#

#
# < CONFIG Link Option
#
CONFIG_IMG1_FLASH=y
# CONFIG_IMG1_SRAM is not set
# CONFIG_CODE_XIP_DATA_PSRAM is not set
# CONFIG_CODE_PSRAM_DATA_SRAM is not set
# CONFIG_CODE_PSRAM_DATA_PSRAM is not set
CONFIG_CODE_XIP_DATA_SRAM=y

#
# < CONFIG Mass Production
#
# CONFIG_MP_INCLUDED is not set

#
# < CONFIG Shell CMD
#
CONFIG_SUPPORT_ATCMD=y
CONFIG_ATCMD_IO_LOGUART=y
# CONFIG_ATCMD_IO_UART is not set
# CONFIG_LONGER_CMD is not set

#
# < CONFIG VFS
#
CONFIG_VFS_LITTLEFS_INCLUDED=y
# CONFIG_VFS_FATFS_INCLUDED is not set

#
# < CONFIG OTA OPTION
#
# CONFIG_UPGRADE_BOOTLOADER is not set
# CONFIG_COMPRESS_OTA_IMG is not set

#
# < CONFIG Application Processor
#
CONFIG_AP_CORE_KM4=y
# CONFIG_AP_CORE_KR4 is not set
CONFIG_WIFI_INIC=y

#
# < CONFIG BT
#
# CONFIG_BT_MENU is not set

#
# < CONFIG WIFI
#
CONFIG_WLAN_MENU=y
CONFIG_WLAN_KM4_AP_KR4_NP=y
# CONFIG_WLAN_KR4_AP_KM4_NP is not set
# CONFIG_WIFI_KM4_SINGLE_CORE is not set
# CONFIG_WIFI_KR4_SINGLE_CORE is not set
# CONFIG_HIGH_TP_TEST is not set
CONFIG_WLAN=y
CONFIG_AS_INIC_NP=y
CONFIG_FW_DRIVER_COEXIST=y
# CONFIG_WIFI_CSI_ENABLE is not set
# CONFIG_WIFI_ANTDIV_ENABLE is not set
# CONFIG_WIFI_11K_ENABLE is not set
# CONFIG_WIFI_11V_ENABLE is not set
# CONFIG_WIFI_11R_ENABLE is not set

#
# < CONFIG LWIP
#
# CONFIG_LWIP_DEBUG is not set

#
#

#
# < MENUCONFIG FOR KR4 CONFIG
#

#
# < CONFIG CHIP
#
CONFIG_AMEBALITE=y
RSICV_CORE_KR4=y
# CONFIG_FPGA is not set
CONFIG_AMEBALITE_A_CUT=y

#
# < CONFIG OS
#
CONFIG_KERNEL=y
PLATFORM_FREERTOS=y
TASK_SCHEDULER_DISABLED=0

#
# < CONFIG FUNCTION TEST
#
# CONFIG_PER_TEST is not set

#
# < SSL Config
#
CONFIG_USE_MBEDTLS_ROM=y
CONFIG_MBED_TLS_ENABLED=y
# CONFIG_SSL_ROM_TEST is not set

#
# < Audio Config
#
# CONFIG_AUDIO_FWK is not set
# CONFIG_MEDIA_PLAYER is not set

#
# Third Party Lib
#
# CONFIG_SPEEX_LIB is not set
# CONFIG_OPUS_LIB is not set

#
# < CONFIG IPC Message Queue
#
# CONFIG_IMQ_EN is not set

#
# < To set debug msg flag
#
# CONFIG_DEBUG_RXI_DUMP is not set
CONFIG_DEBUG_BACK_TRACE=y
CONFIG_DEBUG_CRASH_DUMP=y
CONFIG_STACK_DUMP_DEPTH=100
CONFIG_DEBUG_CRASH_DUMP_RUNS=1

#
# < Build Option
#
CONFIG_TOOLCHAIN_VSDK=y
# CONFIG_LINK_ROM_LIB is not set
CONFIG_LINK_ROM_SYMB=y

#
